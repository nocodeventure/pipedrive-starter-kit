{
  "name": "example-app-todo-list",
  "version": "1.0.0",
  "description": "Simple todo list app for Pipedrive",
  "main": "server.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "react-scripts build && nodemon server.js",
    "dev": "concurrently \"npm run react:start\" \"nodemon server.js\"",
    "react:start": "set PORT=3001 && react-scripts start",
    "react:build": "react-scripts build",
    "db:generate": "npx drizzle-kit generate",
    "db:migrate": "npx drizzle-kit migrate",
    "db:push": "npx drizzle-kit push",
    "db:studio": "npx drizzle-kit studio",
    "db:apply-rls": "node database/apply-rls.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nocodeventure/pipedrive-starter-kit.git"
  },
  "keywords": [],
  "author": "",
  "bugs": {
    "url": "https://github.com/nocodeventure/pipedrive-starter-kit/issues"
  },
  "homepage": "./",
  "dependencies": {
    "@pipedrive/app-extensions-sdk": "^0.1.0",
    "body-parser": "^1.20.0",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.1",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^9.0.3",
    "pipedrive": "^17.1.2",
    "postgres": "^3.4.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "drizzle-kit": "^0.31.8",
    "nodemon": "^3.1.11"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:3000"
}